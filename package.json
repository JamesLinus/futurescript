{
    "name": "futurescript",
    "version": "0.2.0",
    "description": "A future-style language that compiles to JS.",
    "keywords": ["fus", "js", "javascript", "language", "es6", "futurescript"],
    "author": "Zhenzhen Zhan <zhanzhenzhen@hotmail.com>",
    "homepage": "http://futurescript.org/",
    "license": "SEE LICENSE IN LICENSE.txt",
    "repository": {
        "type": "git",
        "url": "https://github.com/zhanzhenzhen/futurescript.git"
    },
    "dependencies": {
        "esprima": "2.x",
        "babel-core": "6.x",
        "babel-preset-es2015": "6.x"
    },
    "devDependencies": {
        "babel-cli": "6.x"
    },
    "engines": {
        "node": ">=0.12.0"
    },
    "bin": {
        "fus": "./bin/fus"
    },
    "preferGlobal": true,
    "scripts": {
        "postinstall": "./target/fill-ref.js"
    },
    "babel": {
        "plugins": [
            "transform-es2015-modules-commonjs",
            "transform-es2015-destructuring",
            "transform-es2015-parameters"
        ]
    }
}
